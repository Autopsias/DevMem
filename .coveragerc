[run]
source = .
omit = 
    tests/*
    scripts/*
    .claude/*
    .mypy_cache/*
    __pycache__/*
    *.pyc
    setup.py
    venv/*
    .venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

show_missing = True
skip_covered = False
precision = 2

[html]
directory = htmlcov
title = DevMem Test Coverage Report